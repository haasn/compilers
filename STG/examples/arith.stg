zero = \  z s -> z ;
succ = \n z s -> s n ;

add = \x y -> x y (\n -> succ (add n y))  ;
mul = \x y -> x x (\n -> add y (mul n y)) ;

eq = \x y -> x (y true (const false)) (\n -> y false (\m -> eq n m)) ;

toInt = \x -> x #0 (\n -> + #1 (toInt n)) ;

one   = succ zero ;
two   = succ one  ;
three = succ two
