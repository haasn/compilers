map = \f l ->
  case l of {
    <0> -> l

    <1> x xs -> let fx  = @ f x      ;
                    mfx = @ map f xs
		in <1> fx mfx
  };

map1 = \f ->
  let mf = \xs ->
    case xs of {
      <0> -> l
      <1> y ys -> let fy  = @ f y   ;
	              mfy = @ mf ys
		  in <1> fy mfy
    }
  in mf
